<Window x:Class="POP_SF49_16GUI.GUI.UserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POP_SF49_16GUI.GUI"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        Title="Salon namestaja" Height="600" Width="500">
    <Border Padding="6">
        <StackPanel>
            <ToolBarTray>
                <ToolBar>
                    <Button Name="btnKupi" ToolTip="Dodaj u korpu">
                        <Image Source="/Resources/Change.bmp" />
                    </Button>
                </ToolBar>
            </ToolBarTray>
            <Border Background="DarkSlateBlue">
                <DataGrid IsReadOnly="True" CanUserAddRows="False" Height="300" Width="auto" Margin="10" Name="dgPrikaz" SelectedItem="{Binding Path=Objekat}">

                </DataGrid>
            </Border>
        </StackPanel>
    </Border>
</Window>
