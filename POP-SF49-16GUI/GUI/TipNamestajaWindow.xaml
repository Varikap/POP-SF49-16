<Window x:Class="POP_SF49_16GUI.GUI.TipNamestajaWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POP_SF49_16GUI.GUI"
        mc:Ignorable="d"
        Title="Tip Namestaja" Height="150" Width="300">
    <Border Padding="1">
        <StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label FontSize="14" Margin="15" Content="Naziv"/>
                <TextBox Name="tbNaziv" FontSize="14" Width="200" Margin="2 15 2 17" Padding="2" Text="{Binding Path=Naziv}"></TextBox>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <Button Name="btnBack" Click="btnBack_Click" Content="Back" Margin="20" Width="40" />
                <Button Name="btnSubmit" Click="btnSubmit_Click" Content="Submit" Margin="80 20 20 20" Width="80" />
            </StackPanel>
        </StackPanel>
    </Border>
</Window>
